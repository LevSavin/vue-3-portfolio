        "volume":{"minimum":1000,"maximum":100000,"type":"number"},
        "email":{"type":"email","required":true},
        "password":{"type":"password","required":true},
        "phone":{"type":"phone"},
        "website":{"type":"url"}
      },
      "required": ["name", "maxWeights", "maxPallets"]
    }
  }
}